        "direct",
        "auto",
		"proxy",
        "{all}"
      ]
    },
    {
      "tag":"📺 Bilibili",
      "type":"selector",
      "outbounds":[
        "direct",
		"auto"

      ]
    },
    {
      "tag":"🎬 MediaVideo",
      "type":"selector",
      "outbounds":[
        "proxy",
		"auto",
        "direct",
        "{all}"
      ]
    },
    {
      "tag":"🌏 !cn",
      "type":"selector",
      "outbounds":[
        "proxy",
		"auto",
        "{all}"
		"direct"
      ]
    },
    {
      "tag":"🌏 cn",
      "type":"selector",
      "outbounds":[
        "direct",
        "proxy"
      ]
    },
    {
      "tag":"🛑 AdBlock",
      "type":"selector",
      "outbounds":[
        "block",
        "direct"
      ]
    },
    {
      "tag":"auto",
      "type":"urltest",
      "outbounds":[
        "{all}"
      ],
      "filter":[
        {"action":"exclude","keywords":["网站|剩余|流量|过期|时间|期|地址"]}
      ],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "block",
      "tag": "block"
    }
  ],
